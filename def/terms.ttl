# Mereological (Compositional) Dimensions

:Monograph a owl:Class;
    owl:equivalentClass bf:Monograph .

:Composite a owl:Class;
    ptg:abstract true .

:Part a owl:Class .

:Archival a owl:Class;
    rdfs:label "Samling"@sv;
    rdfs:subClassOf :Composite;
    owl:equivalentClass bf:Archival .

:Collection a owl:Class;
    rdfs:label "Samling"@sv;
    rdfs:subClassOf :Composite;
    owl:equivalentClass dctype:Collection, bf:Collection .

:Integrating a owl:Class;
    rdfs:subClassOf :Composite;
    owl:equivalentClass bf:Integrating .

:MonographicComponentPart a owl:Class;
    rdfs:subClassOf :Part .

:Multipart a owl:Class;
    rdfs:subClassOf :Composite .

:MultipartMonograph a owl:Class;
    rdfs:subClassOf :Multipart, :Monograph;
    owl:equivalentClass bf:MultipartMonograph .

:Serial a owl:Class;
    rdfs:subClassOf :Composite;
    owl:equivalentClass bf:Serial .

# Manifestation Dimensions

:Digital a owl:Class;
    ptg:abstract true;
    #rdfs:subClassOf [ owl:unionOf (dctype:InteractiveResource dctype:Software) ]
    rdfs:subClassOf :Product;
    owl:equivalentClass bf:Electronic .

:Manuscript a owl:Class;
    rdfs:subClassOf :Product;
    owl:equivalentClass bf:Manuscript .

:Print a owl:Class;
    rdfs:subClassOf :Product;
    owl:equivalentClass bf:Print .

:Tactile a owl:Class;
    rdfs:subClassOf :Product;
    owl:equivalentClass bf:Tactile .

:PhysicalObject a owl:Class;
    rdfs:subClassOf :Product;
    rdfs:label "Föremål"@sv;
    skos:altLabel "Realia"@sv;
    owl:equivalentClass dctype:PhysicalObject .

:Software a owl:Class;
    rdfs:subClassOf :Digital;
    owl:equivalentClass dctype:Software, sdo:SoftwareApplication .

