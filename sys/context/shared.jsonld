{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "sdo": "http://schema.org/",
    "jsonld": "http://www.w3.org/ns/json-ld#",
    "fresnel": "http://www.w3.org/2004/09/fresnel#",
    "kbv": "https://id.kb.se/vocab/",

    "Property": "rdf:Property",
    "Datatype": "rdfs:Datatype",
    "Ontology": "owl:Ontology",
    "Class": "owl:Class",
    "ObjectProperty": "owl:ObjectProperty",
    "DatatypeProperty": "owl:DatatypeProperty",
    "FunctionalProperty": "owl:FunctionalProperty",
    "TransitiveProperty": "owl:TransitiveProperty",
    "Restriction": "owl:Restriction",

    "subClassOf": {"@id": "rdfs:subClassOf", "@container": "@set"},
    "subPropertyOf": {"@id": "rdfs:subPropertyOf", "@container": "@set"},
    "equivalentClass": {"@id": "owl:equivalentClass", "@container": "@set"},
    "equivalentProperty": {"@id": "owl:equivalentProperty", "@container": "@set"},
    "imports": {"@id": "owl:imports",  "@container": "@set"},
    "unionOf": {"@id": "owl:unionOf", "@container": "@list"},
    "intersectionOf": {"@id": "owl:intersectionOf", "@container": "@list"},
    "propertyChainAxiom": {"@id": "owl:propertyChainAxiom", "@container": "@list"},
    "onProperty": {"@id": "owl:onProperty"},
    "inverseOf": {"@id": "owl:inverseOf"},
    "allValuesFrom": {"@id": "owl:allValuesFrom"},
    "someValuesFrom": {"@id": "owl:someValuesFrom"},
    "hasValue": {"@id": "owl:hasValue"},
    "domain": {"@id": "rdfs:domain", "@container": "@set"},
    "range": {"@id": "rdfs:range", "@container": "@set"},
    "domainIncludes": {"@id": "sdo:domainIncludes", "@container": "@set"},
    "rangeIncludes": {"@id": "sdo:rangeIncludes", "@container": "@set"},
    "isDefinedBy": {"@id": "rdfs:isDefinedBy"},
    "seeAlso": {"@id": "rdfs:seeAlso", "@container": "@set"},
    "abstract": "http://protege.stanford.edu/plugins/owl/protege#abstract",
    "termGroup": "http://purl.org/vocab/vann/termGroup",
    "term_status": "http://www.w3.org/2003/06/sw-vocab-status/ns#term_status",
    "lensGroup": "fresnel:group",

    "labelByLang": {"@id": "label", "@container": "@language"},
    "commentByLang": {"@id": "comment", "@container": "@language"},
    "prefLabelByLang": {"@id": "prefLabel", "@container": "@language"},
    "altLabelByLang": {"@id": "altLabel", "@container": "@language"},
    "noteByLang": {"@id": "note", "@container": "@language"},
    "definitionByLang": {"@id": "definition", "@container": "@language"},
    "scopeNoteByLang": {"@id": "scopeNote", "@container": "@language"},
    "changeNoteByLang": {"@id": "changeNote", "@container": "@language"},
    "titleByLang": {"@id": "title", "@container": "@language"},
    "descriptionByLang": {"@id": "description", "@container": "@language"},

    "langTag": {"@id": "code", "@type": "ISO639-1"},
    "langCode": {"@id": "code", "@type": "ISO639-2"},
    "langCodeBib": {"@id": "code", "@type": "ISO639-2-B"},
    "langCodeTerm": {"@id": "code", "@type": "ISO639-2-T"},
    "langCodeFull": {"@id": "code", "@type": "ISO639-3"}
  }
}
