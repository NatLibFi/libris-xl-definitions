{
  "base": "http://id.kb.se/",
  "documentUriTemplate": "{+thing}?data",
  "documentThingLink": "about",
  "alphabet": "0123456789bcdfghjklmnpqrstvwxz",
  "randomVariable": "randomKey",
  "maxRandom": 899,
  "timestampVariable": "timeKey",
  "epochDate": "2014-01-01",
  "timestampCaesarCipher": true,
  "uuidVariable": null,
  "slugCharInAlphabet": true,
  "slugCase": "lower",
  "compoundSlugSeparator": "",
  "rulesByDataset": {
    "auth": {
      "uriTemplate": "/{+basePath}/{timeKey}-{randomKey}-{compoundSlug}",
      "ruleByBaseType": {
        "Person": {
          "subclasses": ["Persona"],
          "basePath": "person",
          "compoundSlugFrom": [
            [["familyName", "givenName"], "name"], "numeration", "birthYear", "deathYear", "personTitle"
          ]
        },
        "Family": {
          "basePath": "family",
          "compoundSlugFrom": ["name"]
        },
        "Organization": {
          "basePath": "org",
          "compoundSlugFrom": ["name"]
        },
        "Meeting": {
          "basePath": "event",
          "compoundSlugFrom": ["name", "date"]
        },
        "Jurisdiction": {
          "basePath": "gov",
          "compoundSlugFrom": ["name", "isPartOf"]
        },
        "Place": {
          "basePath": "place",
          "compoundSlugFrom": [["prefLabel", "label"]]
        },
        "CreativeWork": {
          "basePath": "work",
          "compoundSlugFrom": [["title", "uniformTitle"], "attributedTo"]
        },
        "Concept": {
          "basePath": "term",
          "compoundSlugFrom": ["prefLabel", "inScheme"]
        },
        "ConceptScheme": {
          "uriTemplate": "/def/scheme/{notation}",
          "variables": ["notation"]
        },
        "*": {
          "uriTemplate": "/auth/{identifier}",
          "thingUriTemplate": "/resource/{+document}"
        }
      }
    },
    "bib": {
      "uriTemplate": "/resource/bib/{timeKey}-{randomKey}",
      "ruleByBaseType": {
        "CreativeWork": {
          "compoundSlugFrom": [["title", "uniformTitle"], "attributedTo"]
        },
        "*": {
          "thingUriTemplate": "/resource/{+document}"
        }
      }
    },
    "hold": {
      "uriTemplate": "/hold/{owner}/{timeKey}-{randomKey}"
    }
  }
}
