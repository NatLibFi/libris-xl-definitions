    },

    "321": {},
    "362": {},

    "440": {"prefer": "490"},

    "500": {"$a": {"domainEntity": "Instance", "property": "note"}},
    "504": {"$a": {"domainEntity": "Instance", "property": "supplementaryContentNote"}},
    "505": {"$a": {"domainEntity": "Instance", "property": "listOfContents"}},
