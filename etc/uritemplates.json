[
  {
    "matchType": "Record",
    "uriTemplate": "/resource/bib/{controlNumber}"
  },
  {
    "matchType": "Country",
    "uriTemplate": "/def/countries/{notation}"
  },
  {
    "matchType": "Language",
    "uriTemplate": "/def/languages/{notation}", "matchUriToken": "^\\w{2,3}$"
  },
  {
    "matchType": "IdentifierScheme",
    "uriTemplate": "/def/identifiers/{identifierScheme}"
  },
  {
    "matchType": "Concept",
    "uriTemplate": "http://dewey.info/class/{notation}/e{+inScheme.version}/",
    "match": {
      "inScheme": {
        "notation": "DDC",
        "isVersionOf": {"@id": "http://dewey.info/scheme/ddc/"}
      }
    }
  },
  {
    "matchType": "ConceptScheme",
    "uriTemplate": "http://dewey.info/scheme/edition/e{+version}/",
    "match": {
      "notation": "DDC",
      "isVersionOf": {"@id": "http://dewey.info/scheme/ddc/"}
    }
  },
  {
    "matchType": "Concept",
    "uriTemplate": "/topic/{+inScheme.notation}/{notation}"
  },
  {
    "matchType": "Concept",
    "uriTemplate": "/topic/{+inScheme.notation}/{prefLabel}"
  },
  {
    "matchType": "ConceptScheme",
    "uriTemplate": "/topic/{+notation}"
  },
  {
    "matchType": "Place",
    "uriTemplate": "/place/{+prefLabel}"
  },
  {
    "matchType": "Nationality",
    "uriTemplate": "/def/nationalities/{notation}", "matchUriToken": "^[a-z-]{7}$"
  }
]
