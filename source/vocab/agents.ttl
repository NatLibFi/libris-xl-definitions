:birthDate a owl:DatatypeProperty;
    rdfs:label "födelsedatum"@sv;
    rdfs:domain :Person;
    owl:equivalentProperty sdo:birthDate, madsrdf:birthDate  .

# in favor of lifeSpan.
#:birthYear a owl:DatatypeProperty;
