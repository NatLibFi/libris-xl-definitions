@prefix : <http://id.kb.se/vocab/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix fresnel: <http://www.w3.org/2004/09/fresnel#> .

:showProperties rdfs:subPropertyOf fresnel:showProperties .

:Resource
    :topConceptOf : ;
    :showProperties ( :prefLabel :label :name :code :isPartOf :isDefinedBy ) .

:Record
    :showProperties ( :code :inDataset ) .

:Instance
    :topConceptOf : ;
    #:displayLens [
    #        :category :chip ;
    #        :showProperties ( :title :publishedYear :instanceOf )
    #    ] , [
    #        :category :card ;
    #        :showProperties ( [ :inverseOf :itemOf ] )
    #    ] ;
    :showProperties ( :mediaType :carrierType :title :publicationYear :identifiedBy :instanceOf ) .

:Work
    :topConceptOf : ;
    :showProperties ( :title :contribution ) .

:Item
    :topConceptOf : ;
    :showProperties ( :itemOf :heldBy ) .

:Title
    :showProperties ( :mainTitle :subtitle :titleDate :partNumber :partTitle ) .

:Identifier
    :topConceptOf : ;
    :showProperties ( :value :note ) .

:ProvisionActivity
    :showProperties ( :date :agent :place ) .

:Person
    :topConceptOf : ;
    :showProperties ( :name :familyName :givenName :birthYear :deathYear :notation :personTitle ) .

:Library
    #:search [ :url "https://bibdb.libris.kb.se/search" ;
    #            :mapping [ :predicate :sigel;
    #                    :variable "sigel" ] ] ;
    :showProperties ( :sigel ) .

:Concept
    :topConceptOf : ;
    :showProperties ( :prefLabel :notation :inScheme :inCollection ) .

:GenreForm
    :topConceptOf : .

:Subject
    :topConceptOf : .

:ConceptScheme
    :showProperties ( :notation ) .

:Ontology
    :showProperties ( :title [ :inverseOf :isDefinedBy ] ) .
