# MARC terms, unstable enums...


marc:fieldref a owl:DatatypeProperty;
    rdfs:label "Länkning"@sv;
    skos:definition "$6 i MARC21"@sv .

marc:groupid a owl:DatatypeProperty;
    rdfs:label "Länk- och sekvensnummer"@sv;
    skos:definition "$8 i MARC21"@sv .

:citationSource a owl:DatatypeProperty;
    rdfs:label "Källa"@sv;
    rdfs:domain marc:SourceDataNotFound .

#TITLE

marc:mediaTerm a owl:DatatypeProperty;
    rdfs:label "Medieterm"@sv;
    rdfs:comment "Medieterm som är del av titel i en MARC21 struktur. används ej i RDA"@sv;
    rdfs:domain :Instance .

marc:nonfilingChars a owl:DatatypeProperty;
    rdfs:label "Fileringsvärde"@sv;
    rdfs:comment "Värde '0' innebär att fileringen skall starta med fältets första alfanumeriska tecken.  Värde '1-9' innebär att fältet inleds med alfanumeriska tecken som skall uteslutas vid filering."@sv;
    rdfs:domainIncludes :Title .

marc:searchElement a owl:DatatypeProperty;
    rdfs:label "Titeln är sökelement"@sv;
    rdfs:comment "Indikatorvärde saknar betydelse i LIBRIS-tillämpningen."@av .

marc:searchControl a owl:ObjectProperty;
    rdfs:label "Titeln blir biuppslag i konventionell katalog"@sv .

#IDENTIFIERS
marc:hiddenValue a owl:DatatypeProperty;
    rdfs:label "Ogiltigt värde"@sv;
    rdfs:domain :Identifier .

#NOTE
:technicalNote a owl:DatatypeProperty;
    rdfs:label "Intern anmärkning av systemteknisk karaktär"@sv .

marc:numberOfReferences a owl:DatatypeProperty;
    rdfs:label "Number of references"@en, "Antal referenser"@sv ;
    rdfs:domain :BibliographyNote .

marc:linkText  a owl:DatatypeProperty;
    rdfs:label "Link text"@en, "Länktext"@sv ;
    rdfs:domain :Endeavour .

#AGENT

marc:numeration a owl:DatatypeProperty;
    rdfs:label "Numeration"@en, "Ordningstal"@sv;
    rdfs:comment "Förekommer endast för namn i rak följd. Ange ordningstal för regenter med romerska siffror."@sv;
    rdfs:domain :Person .

marc:titleOther a owl:DatatypeProperty;
    rdfs:label "Titel eller övrig beteckning"@sv;
    rdfs:domain :Agent .

marc:fullerFormName a owl:DatatypeProperty;
    rdfs:label "Fuller form of name"@en, "Fullständigare namnform"@sv;
    rdfs:domain :Agent .

marc:attributionQualifier a owl:DatatypeProperty;
    rdfs:label "Attribution qualifier"@en, "Attribuerande tillägg"@sv;
    rdfs:domain :Agent .

marc:BiographicalOrHistoricalData a owl:Class;
    rdfs:label "Biografisk information"@sv .

marc:hasOtherAttributes a owl:ObjectProperty;
    rdfs:label "Andra attribut för person- och organisationsnamn"@sv;
    rdfs:domain :Agent;
    rdfs:range marc:OtherAttributes .

marc:OtherAttributes a owl:Class;
    rdfs:label "Andra attribut för person- och organisationsnamn"@sv .


