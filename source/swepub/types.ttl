prefix : <https://id.kb.se/vocab/>
prefix swepub: <https://id.kb.se/term/swepub/>
prefix sppubl: <https://id.kb.se/term/swepub/publication/>
prefix spout: <https://id.kb.se/term/swepub/output/>

swepub:ArtisticWork a :GenreForm ;
    :exactMatch sppubl:kfu, spout:artistic-work .

swepub:Book a :GenreForm ;
    :exactMatch sppubl:bok, spout:publication\/book .

swepub:BookChapter a :GenreForm ;
    :exactMatch sppubl:kap, spout:publication\/book-chapter .

swepub:ConferencePaper a :GenreForm ;
    :closeMatch sppubl:kon\/ref, spout:conference\/paper .

swepub:DoctoralThesis a :GenreForm ;
    :exactMatch sppubl:dok, spout:publication\/doctoral-thesis .

swepub:EditorialCollection a :GenreForm ;
    :exactMatch sppubl:sam, spout:publication\/edited-book .

swepub:EditorialProceedings a :GenreForm ;
    :exactMatch sppubl:pro, spout:conference\/proceeding .

swepub:JournalArticle a :GenreForm ;
    :closeMatch sppubl:art\/ref, spout:publication\/journal-article .

swepub:LicentiateThesis a :GenreForm ;
    :exactMatch sppubl:lic, spout:publication\/licentiate-thesis .

swepub:OtherPublication a :GenreForm ;
    :exactMatch sppubl:ovr, spout:publication  .

swepub:Patent a :GenreForm ;
    :exactMatch sppubl:pat, spout:intellectual-property\/patent .

swepub:Report a :GenreForm ;
    :exactMatch sppubl:rap, spout:publication\/report .

swepub:ResearchReview a :GenreForm ;
    :exactMatch sppubl:for, spout:publication\/review-article .

swepub:Review a :GenreForm ;
    :exactMatch sppubl:rec, spout:publication\/book-review .

