prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix schema: <http://schema.org/>
prefix : <https://id.kb.se/vocab/>
prefix swepub: <https://id.kb.se/term/swepub/>

swepub:PublicationStatus a :EnumerationClass ;
    rdfs:subClassOf :Note ;
    :category :pending .

swepub:PublicationType a :EnumerationClass ;
    rdfs:subClassOf :GenreForm ;
    :category :pending .

swepub:OutputType a :EnumerationClass ;
    rdfs:subClassOf :GenreForm ;
    :category :pending .

swepub:Published a swepub:PublicationStatus .
swepub:Published a swepub:PublicationStatus .
swepub:Preprint a swepub:PublicationStatus, swepub:OutputType .
swepub:Submitted a swepub:PublicationStatus .
swepub:Accepted a swepub:PublicationStatus .
swepub:InPress a swepub:PublicationStatus .
swepub:EpubAheadOfPrintOnlineFirst a swepub:PublicationStatus .

swepub:ArtisticWork a swepub:PublicationType ;
    :code "/kfu" .
swepub:Book a swepub:PublicationType ;
    :code "bok" .
swepub:BookChapter a swepub:PublicationType ;
    owl:sameAs swepub:svep\/kap ;
    #owl:sameAs swepub:kap ;
    :source swepub:svep ;
    :code "kap" .
swepub:ConferencePaper a swepub:PublicationType ;
    :code "kon" .
swepub:DoctoralThesis a swepub:PublicationType ;
    :code "dok" .
swepub:EditorialCollection a swepub:PublicationType ;
    :code "sam" .
swepub:EditorialProceedings a swepub:PublicationType ;
    :code "pro" .
swepub:JournalArticle a swepub:PublicationType ;
    :code "art" .
swepub:LicentiateThesis a swepub:PublicationType ;
    :code "lic" .
swepub:OtherPublication a swepub:PublicationType ;
    :code "ovr" .
swepub:Patent a swepub:PublicationType ;
    :code "pat" .
swepub:Report a swepub:PublicationType ;
    :code "rap" .
swepub:ResearchReview a swepub:PublicationType ;
    :code "for" .
swepub:Review a swepub:PublicationType ;
    :code "rec" .
